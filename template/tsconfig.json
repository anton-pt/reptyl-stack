{
  "compilerOptions": {
    "noEmit": true,
    "rootDirs": [
      "./"
    ],
    "outDir": "dist",
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true
  }
}